  <types>
    <dataTypes />
    <pous>
      <pou name="BCD_TO_DWORD" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="en">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="bin">
              <type>
                <DWORD />
              </type>
            </variable>
          </inputVars>
          <outputVars>
            <variable name="eno">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="err">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="wout">
              <type>
                <DWORD />
              </type>
            </variable>
          </outputVars>
          <localVars>
            <variable name="digit">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="n">
              <type>
                <DWORD />
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">err := FALSE;
wout := 0;

IF en THEN
	FOR digit := 0 TO 7 DO
		n := SHR(bin AND SHL(16#F, digit * 4), digit * 4);
		IF n &gt; 9 THEN
			err := TRUE;
			RETURN;
		END_IF
		wout := wout + n * LREAL_TO_DWORD(EXPT(10, digit));
	END_FOR
	eno := TRUE;
ELSE
	eno := FALSE;
END_IF</xhtml>
          </ST>
        </body>
        <addData>
          <data name="http://www.3s-software.com/plcopenxml/interfaceasplaintext" handleUnknown="implementation">
            <InterfaceAsPlainText>
              <xhtml xmlns="http://www.w3.org/1999/xhtml">FUNCTION_BLOCK BCD_TO_DWORD
VAR_INPUT
	en: BOOL;
	bin: DWORD;
END_VAR
VAR_OUTPUT
	eno: BOOL;
	err: BOOL;
	wout: DWORD;
END_VAR
VAR
	digit: INT;
	n: DWORD;
END_VAR
</xhtml>
            </InterfaceAsPlainText>
          </data>
